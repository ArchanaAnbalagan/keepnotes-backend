

  {
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/index.js" },
      { "source": "/old-path", "destination": "/new-path" }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }